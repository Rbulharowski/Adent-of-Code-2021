#since you start in the top left, you can only move down and to the right. This is used to start the while loop that will keep going until all possible paths have been calculated
tempadd(0,0,0,1,cavemap[0][1])
tempadd(0,0,1,0,cavemap[1][0])

while len(temporaryitems) != 0:
  checkforlowest()
  #print(newshortestpath, min)
  shortmap[newshortestpath[0]][newshortestpath[1]] = min
  try:
    if shortmap[newshortestpath[0]-1][newshortestpath[1]] == 1e25 and newshortestpath[0] - 1 > -1 and newshortestpath[0]-1 < 100:
      tempadd(newshortestpath[0], newshortestpath[1], newshortestpath[0]-1,newshortestpath[1],min+cavemap[newshortestpath[0]-1][newshortestpath[1]])
  except:
    if 17 == 16:
      print("Confusion")
  try: 
    if shortmap[newshortestpath[0]][newshortestpath[1]+1] == 1e25 and newshortestpath[1] +1 > -1 and newshortestpath[1]+1 < 100:
      tempadd(newshortestpath[0], newshortestpath[1], newshortestpath[0],newshortestpath[1]+1,min+cavemap[newshortestpath[0]][newshortestpath[1]+1])
  except:
    if 17 == 16:
      print("Confusion")
  try:  
    if shortmap[newshortestpath[0]+1][newshortestpath[1]] == 1e25 and newshortestpath[0] +1  > -1 and newshortestpath[0]+1 < 100:
      tempadd(newshortestpath[0], newshortestpath[1], newshortestpath[0]+1,newshortestpath[1],min+cavemap[newshortestpath[0]+1][newshortestpath[1]])
  except:
    if 17 == 16:
      print("Confusion")
  try:  
    if shortmap[newshortestpath[0]][newshortestpath[1]-1] == 1e25 and newshortestpath[1] - 1 > -1 and newshortestpath[1]-1 < 100:
      tempadd(newshortestpath[0], newshortestpath[1], newshortestpath[0],newshortestpath[1]-1,min+cavemap[newshortestpath[0]][newshortestpath[1]-1])
  except:
    if 17 == 16:
      print("Confusion")
  
  i = 0
  while i < len(temporaryitems):
    if temporaryitems[i][1] == newshortestpath:
      temporaryitems.pop(i)
      i -=1
    i += 1

print(shortmap[99][99])
